{
  "name": "application-manager-server",
  "version": "0.1.1-1",
  "main": "server.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openmsupply/application-manager-server"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-resolvers": "^1.17.10",
    "@types/adm-zip": "^0.4.34",
    "@types/bcrypt": "^3.0.0",
    "@types/carbone": "^1.2.0",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.169",
    "@types/luxon": "^1.27.0",
    "@types/marked": "^2.0.3",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/node-schedule": "^1.3.2",
    "@types/pg": "^7.14.4",
    "@types/pluralize": "^0.0.29",
    "@types/rimraf": "^3.0.0",
    "@types/sharp": "^0.27.1",
    "jest": "^26.4.0",
    "markdown-toc": "^1.2.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.4",
    "postgraphile-plugin-nested-mutations": "^1.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^10.1.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "yarn build && cd build && node server.js",
    "build": "./utils/build_all.sh",
    "test": "jest --runInBand",
    "pg": "exec ./utils/postgraphile_launch.sh",
    "pg_permissions": "exec ./utils/postgraphile_launch_permissions.sh",
    "dev_pg": "concurrently --kill-others-on-fail \"yarn dev\"  \"yarn pg\"",
    "dev_pg_permissions": "concurrently --kill-others-on-fail \"yarn dev\"  \"yarn pg_permissions\"",
    "database_init": "./database/initialise_database.sh tmf_app_manager && ./database/insert_data.sh",
    "postdatabase_init": "./database/turn_on_row_level_security.sh && ./database/post_data_insert.sh",
    "build_plugins": "node ./utils/pluginScripts/buildPlugins.js",
    "push_docs": "exec ./utils/push_docs_to_wiki.sh",
    "gui": "cd ./src/modules/expression-evaluator && yarn gui",
    "generate": "graphql-codegen --config codegen.yml",
    "postinstall": "node ./utils/pluginScripts/yarnInstallPlugins.js",
    "snapshot": "ts-node ./database/snapshotCLI.ts",
    "dockerise": "cd docker && ./dockerise.sh",
    "docker_run": "cd docker && ./run.sh",
    "migrate": "ts-node ./database/migrationScript.ts --migrate"
  },
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^6.0.0",
    "@openmsupply/deep-comparison": "^1.0.1",
    "@openmsupply/expression-evaluator": "^1.9.5",
    "adm-zip": "^0.5.5",
    "bcrypt": "^5.0.0",
    "carbone": "^3.2.3",
    "comment-json": "^4.1.0",
    "concurrently": "^5.3.0",
    "deep-equal": "^2.0.5",
    "dotenv": "^10.0.0",
    "fast-csv": "^4.3.6",
    "fastify": "^3.2.1",
    "fastify-cors": "^4.1.0",
    "fastify-multipart": "^3.1.0",
    "fastify-static": "^4.2.2",
    "json-sql-builder2": "^1.0.24",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "map-values-deep": "^1.0.2",
    "nanoid": "^3.1.21",
    "node-schedule": "^2.0.0",
    "pg": "^8.4.1",
    "pg-diff-cli": "^2.1.0",
    "pluralize": "^8.0.0",
    "postgraphile": "^4.8.0",
    "postgraphile-plugin-connection-filter": "^2.0.0",
    "sharp": "^0.29.3"
  }
}
